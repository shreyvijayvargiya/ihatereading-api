{
	"functions": {
		"index.js": {
			"runtime": "node@18.x",
			"includeFiles": "node_modules/puppeteer-extra*/**"
		},
		"api/**.js": {
			"runtime": "node@18.x",
			"includeFiles": "node_modules/puppeteer-extra*/**"
		}
	},
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/index.js"
		}
	]
}
